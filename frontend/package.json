{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "npx vue-cli-service serve ./code/src/main.ts",
        "build": "npx vue-cli-service build ./code/src/main.ts",
        "lint": "npx vue-cli-service lint ./code/src/main.ts"
    },
    "dependencies": {
        "core-js": "^3.19.3",
        "vue": "^3.2.26",
        "vue-router": "4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "^4.5.15",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-service": "^4.5.13",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.4.1",
        "eslint-plugin-vue": "^8.2.0",
        "typescript": "~4.5.3"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended",
            "@vue/typescript"
        ],
        "parserOptions": {
            "parser": "@typescript-eslint/parser"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
